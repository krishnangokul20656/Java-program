import java.util.*;
public class oddoreven{
    public static void main(String[]args){
        Scanner kbd=new Scanner(System.in);
        int n=kbd.nextInt();
        if(n%2==0){
            System.out.print("The number is even ");
        }
        else
        {
            System.out.print("The number is odd ");
        }
    }
}
